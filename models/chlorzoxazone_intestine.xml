    <listOfRules>
      <assignmentRule name="absorption chlorzoxazone" variable="absorption">
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <times/>
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> Vgu </ci>
                <ci> Ka_abs_czx </ci>
              </apply>
