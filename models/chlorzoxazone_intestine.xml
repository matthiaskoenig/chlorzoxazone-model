            </apply>
            <ci> czx_lumen </ci>
          </apply>
        </math>
      </assignmentRule>
      <rateRule variable="PODOSE_ethanol">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <apply>
              <minus/>
              <ci> ETHABS </ci>
            </apply>
            <ci> Mr_ethanol </ci>
          </apply>
        </math>
      </rateRule>
